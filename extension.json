                "cpd-api-save-description-pages-error-message",
                "cpd-api-load-diagram-success-message",
                "cpd-api-load-diagram-error-message",
                "cpd-api-load-diagram-missingtitle-error-message",
                "cpd-error-message-not-process",
                "cpd-error-message-missing-config",
                "cpd-error-message-missing-element-link",
                "cpd-error-message-mismatched-element-link",
                "cpd-error-message-missing-label",
                "cpd-error-message-saving-diagram",
                "cpd-error-message-missing-root-element",
